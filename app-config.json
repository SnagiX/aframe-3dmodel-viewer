{
    "hostname": "127.0.0.1",
    "port": 36364,
    "ext": [".gltf", ".glb"],
    "defaultRoom": "standard",
    "folders": {
        "models": "/models/",
        "pages": "/pages/"
    },
    "rooms": {
        "standard": {
            "a_scene": {
                "render": "antialias: true; foveationLevel: 1; highRefreshRate: true; physicallyCorrectLights: true; highRefreshRate: true; foveationLevel: 3; ",
                "fog": true,
                "physics": true,
                "timeout": 5000
            },
            "a_text": {
                "font": "https://cdn.aframe.io/fonts/DejaVu-sdf.fnt"
            },
            "a_sky": {
                "material": "color: #fcd6ff"
            },
            "models": {
                "animation__rotation": "property: rotation; easing: linear; to: 0 359 0; loop: true; dur: 9000"
            }
        }
    }
}